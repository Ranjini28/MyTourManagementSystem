@model MyTourManagementMVC.Models.TravelAgencyDetails

@{
    ViewData["Title"] = "Create";
}
<html>
    <head>
        <style>
        body {
        }
        </style>
        </head>
    <body>  
<h1>BonVoyage Exploria</h1>

<h4>Add Travel Agency Details</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            
            <div class="form-group">
                <label asp-for="AgencyName" class="control-label"></label>
                <input asp-for="AgencyName" class="form-control" />
                <span asp-validation-for="AgencyName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="accomodation" class="control-label"></label>
                <input asp-for="accomodation" class="form-control" />
                <span asp-validation-for="accomodation" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TravelMode" class="control-label"></label>
                <input asp-for="TravelMode" class="form-control" />
                <span asp-validation-for="TravelMode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
           </div>
</div>
<br />
<br />
<input type="button" value="Back To Admin" id="mainbtn" class="btn btn-primary" onclick="location.href='@Url.Action("Index", "Admin")'" />

<div>
    <a asp-action="Index">Display All Travel Agency Details</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>